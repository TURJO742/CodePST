#include <stdio.h>
int main()
{
    int n;
    printf("How many numbers you have :");
    scanf("%d", &n);
    int input[n];
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &input[i]);
    }
    for (int i = 0; i < n; i++)
    {
        printf("Numbers : %d\n ", input[i]);
    }
    int number;
    scanf("%d", &number);
    int flag = 0;
    for (int i = 0; i < n; i++)
    {
        if (number == input[i])
        {
            printf("Found/n");
            flag == i;
        }
    }
    if (flag == 0)
    {
        printf("Not Found\n");
    }
}
